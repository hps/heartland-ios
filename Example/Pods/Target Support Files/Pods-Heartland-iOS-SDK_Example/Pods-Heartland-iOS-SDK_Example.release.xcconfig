GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Heartland-iOS-SDK" "${PODS_ROOT}/Headers/Public/XMLDictionary"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/Heartland-iOS-SDK" "$PODS_CONFIGURATION_BUILD_DIR/XMLDictionary"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Heartland-iOS-SDK" -isystem "${PODS_ROOT}/Headers/Public/XMLDictionary"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Heartland-iOS-SDK" -l"XMLDictionary" -framework "UIKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
